{
    "name": "asmblah/fast-cgi",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Asmblah\\FastCgi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asmblah\\FastCgi\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-pcntl": "*",
        "hollodotme/fast-cgi-client": "^3.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
